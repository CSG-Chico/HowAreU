<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.MainMenuActivity"
    android:orientation="vertical"
    android:background="@drawable/bg">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#00000000">
        <include layout="@layout/actionbar_main_menu"></include>
    </androidx.appcompat.widget.Toolbar>

    <androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">


        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="413dp"
            android:layout_height="83dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/whiteborder" />

        <ImageView
            android:id="@+id/moodscale"
            android:layout_width="150dp"
            android:layout_height="170dp"
            android:layout_marginTop="28dp"
            app:layout_constraintEnd_toStartOf="@+id/journal"
            app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/box1" />

        <ImageView
            android:id="@+id/journal"
            android:layout_width="150dp"
            android:layout_height="170dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/box1" />

        <ImageView
            android:id="@+id/wheel"
            android:layout_width="150dp"
            android:layout_height="170dp"
            app:layout_constraintEnd_toStartOf="@+id/breathing"
            app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/moodscale"
            app:srcCompat="@drawable/box1" />

        <ImageView
            android:id="@+id/breathing"
            android:layout_width="150dp"
            android:layout_height="170dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/journal"
            app:srcCompat="@drawable/box1" />

        <ImageView
            android:id="@+id/music"
            android:layout_width="150dp"
            android:layout_height="170dp"
            app:layout_constraintEnd_toStartOf="@+id/quotes"
            app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/wheel"
            app:srcCompat="@drawable/box1" />

        <ImageView
            android:id="@+id/quotes"
            android:layout_width="150dp"
            android:layout_height="170dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/breathing"
            app:srcCompat="@drawable/box1" />

        <ImageView
            android:id="@+id/calendar"
            android:layout_width="65dp"
            android:layout_height="75dp"
            android:layout_marginTop="22dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.158"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView4"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/calendar" />

        <ImageView
            android:id="@+id/home"
            android:layout_width="65dp"
            android:layout_height="75dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView4"
            app:layout_constraintEnd_toEndOf="@+id/imageView4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView4"
            app:layout_constraintVertical_bias="0.652"
            app:srcCompat="@drawable/home"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/history"
            android:layout_width="78dp"
            android:layout_height="74dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="44dp"
            app:layout_constraintEnd_toEndOf="@+id/imageView4"
            app:layout_constraintHorizontal_bias="0.996"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView4"
            app:srcCompat="@drawable/history" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mood Scale"
            android:textColor="@color/white"
            android:textColorHighlight="@color/black"
            android:textColorLink="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/moodscale"
            app:layout_constraintEnd_toEndOf="@+id/moodscale"
            app:layout_constraintStart_toStartOf="@+id/moodscale"
            app:layout_constraintTop_toTopOf="@+id/moodscale" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mood Journal"
            android:textColor="@color/white"
            android:textColorHighlight="@color/black"
            android:textColorLink="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/journal"
            app:layout_constraintEnd_toEndOf="@+id/journal"
            app:layout_constraintStart_toStartOf="@+id/journal"
            app:layout_constraintTop_toTopOf="@+id/journal" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Breathing"
            android:textColor="@color/white"
            android:textColorHighlight="@color/black"
            android:textColorLink="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/breathing"
            app:layout_constraintEnd_toEndOf="@+id/breathing"
            app:layout_constraintHorizontal_bias="0.51"
            app:layout_constraintStart_toStartOf="@+id/breathing"
            app:layout_constraintTop_toBottomOf="@+id/journal"
            app:layout_constraintVertical_bias="0.421" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Exercise"
            android:textColor="@color/white"
            android:textColorHighlight="@color/black"
            android:textColorLink="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/breathing"
            app:layout_constraintEnd_toEndOf="@+id/breathing"
            app:layout_constraintHorizontal_bias="0.413"
            app:layout_constraintStart_toStartOf="@+id/breathing"
            app:layout_constraintTop_toBottomOf="@+id/textView6"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Activity Wheel"
            android:textColor="@color/white"
            android:textColorHighlight="@color/black"
            android:textColorLink="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/wheel"
            app:layout_constraintEnd_toEndOf="@+id/wheel"
            app:layout_constraintStart_toStartOf="@+id/wheel"
            app:layout_constraintTop_toBottomOf="@+id/moodscale" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Quotes"
            android:textColor="@color/white"
            android:textColorHighlight="@color/black"
            android:textColorLink="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/music"
            app:layout_constraintEnd_toEndOf="@+id/music"
            app:layout_constraintStart_toStartOf="@+id/music"
            app:layout_constraintTop_toBottomOf="@+id/wheel" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Music"
            android:textColor="@color/white"
            android:textColorHighlight="@color/black"
            android:textColorLink="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/quotes"
            app:layout_constraintEnd_toEndOf="@+id/quotes"
            app:layout_constraintStart_toStartOf="@+id/quotes"
            app:layout_constraintTop_toBottomOf="@+id/breathing" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</LinearLayout>