<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#876C5B"
    tools:context=".activity.MoodCauseActivity">

    <ImageView
        android:id="@+id/prev"
        android:layout_width="35dp"
        android:layout_height="45dp"
        android:background="@drawable/lightforbutton"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.079"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.043"
        app:srcCompat="@drawable/prev"
        android:contentDescription="TODO"
        tools:ignore="ContentDescription" />

    <Button
        android:id="@+id/savebutton"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:background="@drawable/lightforbutton"
        android:text="Save"
        android:textColorHighlight="@color/black"
        android:textColorLink="@color/white"
        android:textSize="19sp"
        app:fontFamily="sans-serif-medium"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.922"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.042" />


    <ImageView
        android:id="@+id/moodPic"
        android:layout_width="100dp"
        android:layout_height="120dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.155" />

    <TextView
        android:id="@+id/questmood"
        android:layout_width="323dp"
        android:layout_height="30dp"
        android:text="What activities make this feeling?"
        android:textColor="@color/white"
        android:textColorHighlight="@color/black"
        android:textColorLink="@color/white"
        android:textSize="15sp"
        app:fontFamily="serif"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/moodPic"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="115dp"
        android:layout_height="34dp"
        android:text="Activities:"
        android:textColor="@color/white"
        android:textColorHighlight="@color/black"
        android:textColorLink="@color/white"
        android:textSize="15sp"
        app:fontFamily="serif"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.101"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/questmood"
        app:layout_constraintVertical_bias="0.057" />

    <ImageView
        android:id="@+id/weatheract"
        android:layout_width="356dp"
        android:layout_height="137dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/lightforbutton" />

    <TextView
        android:id="@+id/weath"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Weather"
        android:textColorHighlight="@color/black"
        android:textColorLink="@color/white"
        android:textSize="19sp"
        app:fontFamily="sans-serif-medium"
        app:layout_constraintBottom_toBottomOf="@+id/weatheract"
        app:layout_constraintEnd_toEndOf="@+id/weatheract"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="@+id/weatheract"
        app:layout_constraintTop_toTopOf="@+id/weatheract"
        app:layout_constraintVertical_bias="0.09" />

    <ImageView
        android:id="@+id/sunny"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/weatheract"
        app:layout_constraintEnd_toEndOf="@+id/weatheract"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="@+id/weatheract"
        app:layout_constraintTop_toBottomOf="@+id/weath"
        app:layout_constraintVertical_bias="0.176"
        app:srcCompat="@drawable/sunny" />

    <ImageView
        android:id="@+id/rainy"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/sunny"
        app:layout_constraintEnd_toEndOf="@+id/weatheract"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toEndOf="@+id/sunny"
        app:layout_constraintTop_toTopOf="@+id/sunny"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/rainy" />

    <ImageView
        android:id="@+id/cloudy"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/rainy"
        app:layout_constraintEnd_toEndOf="@+id/weatheract"
        app:layout_constraintHorizontal_bias="0.148"
        app:layout_constraintStart_toEndOf="@+id/rainy"
        app:layout_constraintTop_toTopOf="@+id/rainy"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/cloudy" />

    <ImageView
        android:id="@+id/windy"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginEnd="92dp"
        app:layout_constraintBottom_toBottomOf="@+id/cloudy"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.846"
        app:layout_constraintStart_toEndOf="@+id/cloudy"
        app:layout_constraintTop_toTopOf="@+id/cloudy"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/windy" />

    <ImageView
        android:id="@+id/socact"
        android:layout_width="356dp"
        android:layout_height="137dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weatheract"
        app:layout_constraintVertical_bias="0.098"
        app:srcCompat="@drawable/lightforbutton" />

    <TextView
        android:id="@+id/soc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Socials"
        android:textColorHighlight="@color/black"
        android:textColorLink="@color/white"
        android:textSize="19sp"
        app:fontFamily="sans-serif-medium"
        app:layout_constraintBottom_toBottomOf="@+id/socact"
        app:layout_constraintEnd_toEndOf="@+id/socact"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="@+id/socact"
        app:layout_constraintTop_toTopOf="@+id/socact"
        app:layout_constraintVertical_bias="0.09" />

    <ImageView
        android:id="@+id/fam"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/socact"
        app:layout_constraintEnd_toEndOf="@+id/socact"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="@+id/socact"
        app:layout_constraintTop_toBottomOf="@+id/soc"
        app:layout_constraintVertical_bias="0.235"
        app:srcCompat="@drawable/family" />

    <ImageView
        android:id="@+id/friends"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/fam"
        app:layout_constraintEnd_toEndOf="@+id/socact"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toEndOf="@+id/fam"
        app:layout_constraintTop_toTopOf="@+id/fam"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/friends" />

    <ImageView
        android:id="@+id/beloved"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/friends"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.534"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/friends"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/beloved" />

    <ImageView
        android:id="@+id/stranger"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/beloved"
        app:layout_constraintEnd_toEndOf="@+id/socact"
        app:layout_constraintHorizontal_bias="0.236"
        app:layout_constraintStart_toEndOf="@+id/beloved"
        app:layout_constraintTop_toTopOf="@+id/beloved"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/stranger" />

    <ImageView
        android:id="@+id/locact"
        android:layout_width="356dp"
        android:layout_height="137dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/socact"
        app:srcCompat="@drawable/lightforbutton" />

    <TextView
        android:id="@+id/loc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Location"
        android:textColorHighlight="@color/black"
        android:textColorLink="@color/white"
        android:textSize="19sp"
        app:fontFamily="sans-serif-medium"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/locact"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="@+id/locact"
        app:layout_constraintTop_toTopOf="@+id/locact"
        app:layout_constraintVertical_bias="0.105" />

    <ImageView
        android:id="@+id/house"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/locact"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="@+id/locact"
        app:layout_constraintTop_toBottomOf="@+id/loc"
        app:layout_constraintVertical_bias="0.285"
        app:srcCompat="@drawable/house" />

    <ImageView
        android:id="@+id/school"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/house"
        app:layout_constraintEnd_toEndOf="@+id/locact"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toEndOf="@+id/house"
        app:layout_constraintTop_toTopOf="@+id/house"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/school" />

    <ImageView
        android:id="@+id/mall"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/school"
        app:layout_constraintEnd_toEndOf="@+id/locact"
        app:layout_constraintHorizontal_bias="0.163"
        app:layout_constraintStart_toEndOf="@+id/school"
        app:layout_constraintTop_toTopOf="@+id/school"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/mall" />

    <ImageView
        android:id="@+id/restaurant"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/mall"
        app:layout_constraintEnd_toEndOf="@+id/locact"
        app:layout_constraintHorizontal_bias="0.236"
        app:layout_constraintStart_toEndOf="@+id/mall"
        app:layout_constraintTop_toTopOf="@+id/mall"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/restaurant" />


</androidx.constraintlayout.widget.ConstraintLayout>